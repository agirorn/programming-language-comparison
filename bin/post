#!/usr/bin/env bash

echo js_express_js
curl \
  --request POST \
  --header "Content-Type: application/json" \
  --data '{"key":"'`uuid`'"}' \
  http://localhost/app/js_express_js/insert

echo ""
echo "- - - - - - - - - - - - - - - - - - - - - - - - - -"

